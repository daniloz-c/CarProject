@{
    ViewData["Title"] = "My Site";
}

<div class="text-center">
    <h1 class="display-4">My favorite cars</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<div id="carouselExampleRide" class="carousel slide" data-bs-ride="true">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/images/350z.jfif" class="d-block w-100" alt="..." height="700" width="20">
            <h2>Nissan 350z</h2>
        </div>
        <div class="carousel-item">
            <img src="~/images/screen-shot-2020-07-14-at-3-23-51-pm-1594756404.png" class="d-block w-100" alt="..." height="700" width="20">
            <h2>Mazda RX7</h2>
        </div>
        <div class="carousel-item">
            <img src="~/images/evo.jpg" class="d-block w-100" alt="..." height="700" width="20">
            <h2>Mitsubishi Lancer Evo 4</h2>
        </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleRide" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleRide" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>
